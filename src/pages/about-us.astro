  <div class="about-us-section">
    <div class="container-medium w-container">
      <div class="w-layout-grid about-us-grid">
        <div id="w-node-_039cf7cf-262b-77d9-34dd-4f82af481942-0a804130" class="about-image-wrap">
          <img
            src="images/about-image-1.jpg"
            width="1307"
            alt="Truck"
            sizes="(max-width: 479px) 100vw, (max-width: 991px) 95vw, (max-width: 1439px) 97vw, 1307px"
            data-w-id="f05be6a8-d216-aa3c-1d27-253a23a1af45"
            loading="lazy"
            srcset="images/about-image-1-p-500.jpg 500w, images/about-image-1.jpg 726w"
          />
